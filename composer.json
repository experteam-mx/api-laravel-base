{
    "name": "experteam/api-laravel-base",
    "description": "Library to help with Laravel api Authentication",
    "homepage": "https://github.com/experteam-mx/api-laravel-base",
    "keywords": [
        "laravel",
        "crud",
        "experteam"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ren√°n Rivadeneira",
            "email": "renan.rivadeneira@experteam.com.ec",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "ext-json": "*",
        "illuminate/support": "^8 || ^9 || ^10"
    },
    "autoload": {
        "psr-4": {
            "Experteam\\ApiLaravelBase\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Experteam\\ApiLaravelBase\\ApiLaravelBaseServiceProvider"
            ],
            "aliases": {
                "AccessPermission": "Experteam\\ApiLaravelBase\\Facades\\AccessPermission",
                "ActiveEntities": "Experteam\\ApiLaravelBase\\Facades\\ActiveEntitiesFacade"
            }
        }
    }
}
